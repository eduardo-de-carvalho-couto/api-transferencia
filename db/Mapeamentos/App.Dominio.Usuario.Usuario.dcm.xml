<doctrine-mapping>
    <mapped-superclass name="App\Dominio\Usuario\Usuario">

        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field name="nome" column="nome" type="string" length="45" />

        <field name="email" column="email" type="email" length="45" nullable="false" />

        <field name="senha" column="senha" type="string" length="200" nullable="true" />
        
    </mapped-superclass>
</doctrine-mapping>